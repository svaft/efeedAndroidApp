<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadingEdge="none"
    android:keepScreenOn="true"
    android:visibility="visible"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#6A6A6A"
        android:backgroundTintMode="add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <TextView
            android:id="@+id/tvZLabel2"
            android:layout_width="55dp"
            android:layout_height="96dp"
            android:background="@android:drawable/btn_default_small"
            android:clickable="true"
            android:gravity="center|top"
            android:longClickable="true"
            android:text="home"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="45dp"
            android:layout_height="46dp"
            android:elevation="3dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/tvZLabel2"
            app:layout_constraintEnd_toEndOf="@+id/tvZLabel2"
            app:layout_constraintStart_toStartOf="@+id/tvZLabel2"
            app:srcCompat="@drawable/baseline_home_24" />


        <EditText
            android:id="@+id/tvZLabelGlobal"
            android:layout_width="100dp"
            android:layout_height="0dp"
            android:background="@android:drawable/btn_default_small"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:imeOptions="actionDone"
            android:inputType="numberDecimal"
            android:paddingTop="0dp"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            android:text="0.00"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="34sp"
            android:textStyle="normal"
            android:typeface="normal"
            app:layout_constraintBottom_toBottomOf="@+id/tvZLabel"
            app:layout_constraintStart_toEndOf="@+id/tvXLabel"
            app:layout_constraintTop_toTopOf="@+id/tvZLabel"
            app:layout_constraintVertical_bias="0.6"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <TextView
            android:id="@+id/tvZLabel"
            android:layout_width="130dp"
            android:layout_height="48dp"
            android:background="@android:drawable/btn_default_small"
            android:clickable="true"
            android:gravity="top"
            android:longClickable="true"
            android:paddingTop="0dp"
            android:singleLine="true"
            android:text="0.00"
            android:textAlignment="textEnd"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="34sp"
            app:layout_constraintStart_toEndOf="@+id/tvZLabel2"
            app:layout_constraintTop_toTopOf="@+id/tvZLabel2" />

        <TextView
            android:id="@+id/tvXLabel"
            android:layout_width="130dp"
            android:layout_height="48dp"
            android:background="@android:drawable/btn_default_small"
            android:clickable="true"
            android:gravity="top"
            android:longClickable="true"
            android:paddingTop="0dp"
            android:singleLine="true"
            android:text=" 0.00"
            android:textAlignment="textEnd"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="@+id/tvZLabel2"
            app:layout_constraintStart_toStartOf="@+id/tvZLabel"
            app:layout_constraintTop_toBottomOf="@+id/tvZLabel" />

        <EditText
            android:id="@+id/tvXLabelGlobal"
            android:layout_width="100dp"
            android:layout_height="0dp"
            android:background="@android:drawable/btn_default_small"
            android:cursorVisible="false"
            android:ems="10"
            android:imeOptions="actionDone"
            android:inputType="numberDecimal"
            android:onClick="onEditTextClickSelectAll"
            android:paddingTop="0dp"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            android:text="0.00"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="34sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="@+id/tvXLabel"
            app:layout_constraintStart_toEndOf="@+id/tvXLabel"
            app:layout_constraintTop_toTopOf="@+id/tvXLabel"
            app:layout_constraintVertical_bias="0.6"
            tools:ignore="DuplicateSpeakableTextCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="29dp"
            android:layout_height="20dp"
            android:layout_marginEnd="-34dp"
            android:layout_marginBottom="2dp"
            android:text="0.01"
            android:textAlignment="viewEnd"
            app:layout_constraintBottom_toBottomOf="@+id/tvXLabel"
            app:layout_constraintEnd_toStartOf="@+id/tvXLabel" />

        <TextView
            android:id="@+id/textViewZ"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="2dp"
            android:padding="0dp"
            android:paddingTop="0dp"
            android:singleLine="true"
            android:text="Z:"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="@+id/tvZLabel"
            app:layout_constraintTop_toTopOf="@+id/tvZLabel" />

        <TextView
            android:id="@+id/textViewXr"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_marginStart="2dp"
            android:padding="0dp"
            android:paddingTop="0dp"
            android:singleLine="true"
            android:text="R:"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView2"
            app:layout_constraintStart_toStartOf="@+id/tvZLabel"
            app:layout_constraintTop_toBottomOf="@+id/tvZLabel"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="29dp"
            android:layout_height="20dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="2dp"
            android:text="0.05"
            android:textAlignment="viewEnd"
            app:layout_constraintBottom_toBottomOf="@+id/tvZLabel"
            app:layout_constraintStart_toStartOf="@+id/tvZLabel" />

        <com.example.myapplication.CustomViewPager
            android:id="@+id/view_pager"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="4dp"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tabLayout"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout" />


        <com.google.android.material.tabs.TabLayout
            android:id="@+id/tabLayout"
            android:layout_width="0dp"
            android:layout_height="41dp"
            app:layout_constraintEnd_toStartOf="@+id/btStateView"
            app:layout_constraintStart_toEndOf="@+id/constraintLayout3"
            app:layout_constraintTop_toTopOf="parent"
            app:tabIndicatorColor="#FF5722"
            app:tabMode="scrollable"
            app:tabRippleColor="#E61E62"
            app:tabSelectedTextColor="#FF9800" />


        <TextView
            android:id="@+id/tvFeedValue"
            android:layout_width="0dp"
            android:layout_height="19dp"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/buttonTest"
            app:layout_constraintStart_toEndOf="@+id/tvFeedText"
            app:layout_constraintTop_toBottomOf="@+id/tvXLabelGlobal" />

        <TextView
            android:id="@+id/tvFeedText"
            android:layout_width="50dp"
            android:layout_height="19dp"
            android:layout_marginStart="64dp"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:text="feed: "
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/tvXLabel"
            app:layout_constraintTop_toBottomOf="@+id/tvXLabelGlobal" />

        <TextView
            android:id="@+id/tvLogMini"
            android:layout_width="0dp"
            android:layout_height="19dp"
            android:fadingEdge="horizontal"
            android:includeFontPadding="false"
            android:requiresFadingEdge="horizontal"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:singleLine="true"
            android:textColor="@color/white"
            app:layout_constraintEnd_toStartOf="@+id/tvFeedText"
            app:layout_constraintStart_toStartOf="@+id/tvXLabel"
            app:layout_constraintTop_toBottomOf="@+id/tvXLabelGlobal" />

        <ImageView
            android:id="@+id/btStateView"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:background="#FF9800"
            app:layout_constraintBottom_toBottomOf="@+id/tabLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baseline_bluetooth_searching_24" />

        <Button
            android:id="@+id/buttonTest"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="2dp"
            android:insetTop="0dp"
            android:onClick="onJogShow"
            android:text="Button"
            app:layout_constraintBottom_toBottomOf="@+id/tvXLabelGlobal"
            app:layout_constraintStart_toEndOf="@+id/tvZLabelGlobal"
            app:layout_constraintTop_toTopOf="@+id/tvZLabelGlobal" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="450dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvLogMini"
            tools:layout_editor_absoluteX="1dp">

            <ImageButton
                android:id="@+id/btFastTakeAway"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@android:drawable/btn_default"
                android:hapticFeedbackEnabled="true"
                android:longClickable="true"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toTopOf="@+id/guideline98"
                app:layout_constraintEnd_toEndOf="@+id/btFL"
                app:layout_constraintStart_toStartOf="@+id/btFL"
                app:layout_constraintTop_toBottomOf="@+id/btFL"
                app:srcCompat="@drawable/ic_baseline_south_east_24"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/btFB"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@android:drawable/btn_default"
                android:hapticFeedbackEnabled="true"
                android:rotation="0"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toBottomOf="@+id/btFastTakeAway"
                app:layout_constraintEnd_toEndOf="@+id/btFF"
                app:layout_constraintStart_toEndOf="@+id/btFastTakeAway"
                app:layout_constraintTop_toTopOf="@+id/btFastTakeAway"
                app:srcCompat="@drawable/ic_outline_fast_backward_24"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/btMain"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="3dp"
                android:hapticFeedbackEnabled="true"
                android:includeFontPadding="false"
                android:insetTop="0dp"
                android:paddingLeft="5dp"
                android:text="@string/feed"
                android:textSize="16sp"
                android:textStyle="bold"
                app:icon="@drawable/back_outer"
                app:iconPadding="0dp"
                app:iconTint="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/btFB"
                app:layout_constraintEnd_toEndOf="@+id/btPause"
                app:layout_constraintStart_toEndOf="@+id/btFB"
                app:layout_constraintTop_toTopOf="@+id/btFB" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:elevation="3dp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/btPause"
                app:layout_constraintEnd_toEndOf="@+id/btPause"
                app:srcCompat="@drawable/baseline_home_24" />

            <Button
                android:id="@+id/btStop"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginBottom="8dp"
                android:background="#EF9A9A"
                android:backgroundTint="#EF9A9A"
                android:fontFamily="sans-serif-medium"
                android:foregroundTint="#EF9A9A"
                android:onClick="onStop"
                android:text="STOP -------- RESET"
                android:textSize="10sp"
                app:layout_constraintBottom_toTopOf="@+id/btPause"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/btPause"
                app:layout_constraintTop_toTopOf="@+id/btFR" />

            <Button
                android:id="@+id/btPause"
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:hapticFeedbackEnabled="true"
                android:insetTop="0dp"
                android:onClick="onPauseClick"
                android:text="II"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/btFR"
                app:layout_constraintEnd_toEndOf="@+id/btStop"
                app:layout_constraintStart_toEndOf="@+id/btFR" />

            <ImageButton
                android:id="@+id/btFR"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@android:drawable/btn_default"
                android:hapticFeedbackEnabled="true"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toTopOf="@+id/guideline10"
                app:layout_constraintEnd_toStartOf="@+id/guideline7"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/guideline6"
                app:layout_constraintTop_toTopOf="@+id/btFL"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@android:drawable/ic_media_ff"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/btFF"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@android:drawable/btn_default"
                android:hapticFeedbackEnabled="true"
                android:rotation="0"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toTopOf="@+id/guideline10"
                app:layout_constraintEnd_toStartOf="@+id/guideline6"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toTopOf="@+id/guideline50"
                app:srcCompat="@drawable/ic_outline_fast_forward_24"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/btFL"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@android:drawable/btn_default"
                android:backgroundTintMode="src_in"
                android:hapticFeedbackEnabled="true"
                android:longClickable="true"
                android:scaleType="fitCenter"
                app:layout_constraintBottom_toTopOf="@+id/guideline10"
                app:layout_constraintEnd_toStartOf="@+id/guideline"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/guideline50"
                app:srcCompat="@android:drawable/ic_media_rew"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/btChamfer"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="32dp"
                android:background="@android:drawable/btn_default"
                android:hapticFeedbackEnabled="true"
                android:rotation="0"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toTopOf="@+id/btFL"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/ic_untitled__7_"
                tools:ignore="SpeakableTextPresentCheck" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.25" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline98"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.98" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline50"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.5" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.74" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.75" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>